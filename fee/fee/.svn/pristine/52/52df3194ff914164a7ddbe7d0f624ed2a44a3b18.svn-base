/*------搜索框自动补全样式------*/
.searchDis{
    display: inline-block;
    position: relative;
}
.searchDis .show_list{
    position: absolute;
    left: 0;
    top: 25px;
    width:100%;
    background: #fff;
    min-width:150px;
    padding:0;
    z-index: 9999;
}
.searchDis .show_list li{
    line-height: 20px;
    padding: 0 6px;
    list-style:none;
    white-space: nowrap;
}
.show_list li.current{
    color: #fff;
    background: #0059b3;
}

